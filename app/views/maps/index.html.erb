<h1> All Places </h1>
<table>
	<tr> 
		<th>Name </th>
		<th>Latitude </th>
		<th>Longitude </th>
		<th>Action </th>
		
	</tr>
<% @maps.each do |map| %>
	<tr> 
		<td><%= map.name %> </td>
		<td><%= map.latitude %> </td>
		<td><%= map.longitude %></td>
		<td><%= link_to 'Show', map_path(map) %> | <%= link_to 'Edit', edit_map_path(map) %> | <%= link_to 'Delete', map_path(map), method: 'delete' %> </td>
		

	</tr>
<% end %>
</table>
<%= link_to 'New Place', new_map_path, class: 'button radius small' %>